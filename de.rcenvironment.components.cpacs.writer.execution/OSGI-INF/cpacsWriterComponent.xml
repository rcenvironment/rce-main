<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" factory="de.rcenvironment.rce.component" name="CPACS Writer Component">
	
	<implementation class="de.rcenvironment.components.cpacs.writer.execution.CpacsWriterComponent"/>
	
	<service>
		<provide interface="de.rcenvironment.core.component.registration.api.Registerable"/>
	</service>
	
	<property name="rce.component.id" type="String">
		de.rcenvironment.cpacswriter
		de.dlr.sc.chameleon.rce.cpacsdestination.component.Destination_CPACS Saving
	</property>
	
	<property name="rce.component.name" type="String" value="CPACS Writer"/>
	
	<property name="rce.component.group" type="String" value="CPACS"/>
	
	<property name="rce.component.class" type="String" value="de.rcenvironment.components.cpacs.writer.execution.CpacsWriterComponent"/>
	
	<property name="rce.component.inputs" type="String" value="/resources/inputs.json"/>
    <property name="rce.component.outputs" type="String" value="/resources/outputs.json"/>
    <property name="rce.component.configuration" type="String" value="/resources/configuration.json"/> 
	
	<property name="rce.component.icon.bundlename"  type="String" value="de.rcenvironment.components.cpacs.writer.common" />
	<property name="rce.component.icon-16" type="String" value="cpacsWriter16.png"/>
	<property name="rce.component.icon-24" type="String" value="cpacsWriter24.png"/>
    <property name="rce.component.icon-32" type="String" value="cpacsWriter32.png"/>
    <property name="rce.component.version" type="String" value="3.1"/>
    
    <property name="rce.component.size" type="String" value="small"/>
	
</scr:component>
